{
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-undef": 0,
        "comma-dangle": ["error", "never"],
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "arrow-body-style": "off"
    },
    "plugins": [
        "import"
    ]
}
